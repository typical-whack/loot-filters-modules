                "Iron bar",
                "Silver bar",
                "Steel bar",
                "Gold bar",
                "Mithril bar",
                "Adamantite bar",
                "Runite bar",
                "Elemental metal",
                "Primed bar",
                "Primed mind bar",
                "Blurite bar",
                "Enchanted bar",
                "Lovakite bar",
                "'Perfect' gold bar"
            ],
            "default": [
                "Bronze bar",
                "Iron bar",
                "Silver bar",
                "Gold bar"
            ]
        },
